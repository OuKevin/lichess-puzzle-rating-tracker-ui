{
  "name": "lichess-puzzle-rating-tracker-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint \"@(src|test)/**/*.{js,jsx}\"",
    "server": "nodemon server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "^3.4.4",
    "recharts": "^1.8.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!src/index.js"
    ]
  },
  "proxy": "http://localhost:2323/"
}
